{% extends 'base.html.twig' %}

{% block title %}
	{{parent()}}
	- Rums
{% endblock %}

{% block section %}
	<section id="product_section">
		{% for product in products %}
			<div class="product_container">
				<div>{{ product.nom }}</div>
				<div>{{ product.nom }}</div>
				<div>{{ product.description }}</div>
				<div>{{ product.prix }}</div>
				<div>{{ product.nom }}</div>
			</div>
		{% endfor %}
	</section>

{% endblock %}

{% block product_carousel %}
	{% include "/template_parts/_product_slider.html.twig" %}
{% endblock %}

{% block body %}
	<script type="module" src="js/classes/Product.js"></script>
	<script type="module" src="js/modules/_rum.js"></script>
{% endblock %}
